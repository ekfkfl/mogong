<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="scheduleMapper">

	<select id="scheduleData" resultType="taskDTO">
		select title,progress_status as progressStatus,to_char(start_date,'YYYY-MM-DD HH:MM') as startDate,to_char(end_date,'YYYY-MM-DD HH:MM') as endDate from task
	</select>
	
	<insert id="scheduleInsert" parameterType="taskDTO">
	insert into task
	values(task_seq.nextval,#{studyCode},#{title},null,'0001',to_date(#{startDate},'yyyy-mm-dd hh24:mi:ss'),to_date(#{endDate},'yyyy-mm-dd hh24:mi:ss'),null,null,null,null)
</insert>
</mapper>